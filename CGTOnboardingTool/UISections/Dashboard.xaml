<Page x:Class="CGTOnboardingTool.UISections.Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CGTOnboardingTool.UISections"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Dashboard">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="1">
            <ListView x:Name="DashboardReportView" Height="343" Width="790">
                <ListView.View>
                    <GridView x:Name="DashboardReportViewGrid">
                        <GridViewColumn/>
                    </GridView>
                </ListView.View>
            </ListView>
        </Label>


    </Grid>
</Page>
