<Page x:Class="CGTOnboardingTool.UISections.StartUp"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CGTOnboardingTool.UISections"
      mc:Ignorable="d"
      Title="StartUp" Width="1024" Height="720">

    <Grid Width="1024" Height="705">
        
        <Grid Style="{DynamicResource RoundGrid}" Height="690" Width="1001.5">
            <Border Margin="7.5,0,15,15" Background="#C7CADC"/>

            <Button x:Name="btnStartUpNew" Content="New Report" Style="{DynamicResource RoundButton}" Click="btnStartUpNew_Click" Margin="160,320,666,320"/>
            <Button x:Name="btnStartUpImport" Content="Import Report" Style="{DynamicResource RoundButton}" Click="btnStartUpNew_Click" Margin="601,320,160,320" Width="auto" />



        </Grid>
    </Grid>
</Page>
